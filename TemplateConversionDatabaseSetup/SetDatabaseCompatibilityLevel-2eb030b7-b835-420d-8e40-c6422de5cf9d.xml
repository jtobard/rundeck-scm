    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Set Compatibility level to 120</description>
        <fileExtension>bat</fileExtension>
        <script><![CDATA[sqlcmd -S %computername% -Q "ALTER DATABASE %RD_OPTION_CLIENT_CODE%_DEV1_MAIN SET COMPATIBILITY_LEVEL = 120;" 

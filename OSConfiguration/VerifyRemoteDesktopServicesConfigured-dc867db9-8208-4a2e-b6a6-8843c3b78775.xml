    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Verify_Remote_Desktop_Services_configured</description>
        <fileExtension>ps1</fileExtension>
        <script><![CDATA[$obj = gwmi -namespace "Root/CIMV2/TerminalServices" Win32_TerminalServiceSetting

$obj.ChangeMode(4)

$obj.SetSpecifiedLicenseServerList("649604-tidalap4")

$outp = ($obj.GetSpecifiedLicenseServerList()).SpecifiedLSList

Write-Host "Verify Remote Desktop Services Configured..."

If($outp -eq '649604-tidalap4'){
    Write-Host "Set Specified License Server List Successfully !!!"
    Exit 0
}Else{
    Write-Host "Unable to Set Specified License Server List !!"
    Exit 1
}]]></script>
        <scriptargs />
      </command>
    </sequence>
    <uuid>dc867db9-8208-4a2e-b6a6-8843c3b78775</uuid>

      <filter>name: Ansible</filter>
    </nodefilters>
    <nodesSelectedByDefault>true</nodesSelectedByDefault>
    <scheduleEnabled>true</scheduleEnabled>
    <sequence keepgoing='false' strategy='node-first'>
      <command>
        <description>Playbook for verifying FTP access from VM to ftp.crd.com</description>
        <exec>sudo ansible-playbook /etc/ansible/playbooks/Infra/VMCC/VerifyFTPAccess.yml --extra-vars "remote_host=${option.REMOTE_HOST}"</exec>
      </command>
    </sequence>
    <uuid>d6e95657-b064-4075-bbb1-06b49c1b1cc4</uuid>
  </job>
</joblist>
